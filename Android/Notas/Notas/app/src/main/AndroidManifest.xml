<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sdds.react.nativo">
    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Manejo"
        android:usesCleartextTraffic="true">

        <activity android:name=".Trilha.TrilhaList"></activity>

        <activity android:name=".Trilha.Trilha_View"
                  android:noHistory="true"/>

        <activity android:name=".Trilha.Cadastro_trilha"
                  android:noHistory="true" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".Mapa.MapsAct"
            android:label="@string/title_activity_maps" />
        <activity
            android:name=".Initial.MainAct"
            android:label="@string/title_activity_initial_screen_nav"
            android:theme="@style/Manejo.NoActionBar" />
        <activity
            android:name=".Initial.LoginScreenAct"
            android:noHistory="true" />
        <activity
            android:name=".Initial.SplashAct"
            android:noHistory="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>